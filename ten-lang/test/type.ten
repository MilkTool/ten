assert( type( nil )  = 'Nil', "1" )
assert( type( true ) = 'Log', "2" )
assert( type( 123 )  = 'Int', "3" )
assert( type( 1.2 )  = 'Dec', "4" )
assert( type( 'a' )  = 'Sym', "5" )
assert( type( "a" )  = 'Str', "6" )
assert( type( {}  )  = 'Rec', "7" )
assert( type( { .tag: 'Tag' } ) = 'Rec:Tag', "8" )
assert( type( [] nil ) = 'Cls', "9" )
assert( type( fiber[] nil ) = 'Fib', "10" )
assert( type( NULL ) = 'Ptr', "11" )
