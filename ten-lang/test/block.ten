def v1: 'v1'
assert( v1 = 'v1', "1" )

def v2: do
  assert( v1 = 'v1', "2" )
  def v1: 'vv'
  assert( v1 = 'vv', "3" )
for 'v2'

assert( v1 = 'v1', "4" )
assert( v2 = 'v2', "5" )

def v3: do
    set v2: 'vv'
    assert( v2 = 'vv', "6" )
for 'v3'

assert( v2 = 'vv', "7" )
assert( v3 = 'v3', "8" )

def f: [] do
  assert( v1 = 'v1', "9" )
  assert( v2 = 'vv', "10" )
  assert( v3 = 'v3', "11" )
  def v1: 'vv'
  assert( v1 = 'vv', "12" )
  set v3: 'vv'
  assert( v3 = 'vv', "13" )
for 'f'
assert( f() = 'f', "14" )
assert( v1 = 'v1', "15" )
assert( v2 = 'vv', "16" )
assert( v3 = 'vv', "17" )
