def ls:  list( 1, 2, 3, 4, 5 )
def sum: fold( items( ls ), 0, [ a, v ] a + v )
assert( sum = 15, "1" )

def ls: explode( stream( 1, 2, 3, 4, 5 ) )
def sum: fold( items( ls ), 0, [ a, v ] a + v )
assert( sum = 15, "2" )

def ls: cons( 1, cons( 2, cons( 3, cons( 4, cons( 5, nil ) ) ) ) )
def sum: fold( items( ls ), 0, [ a, v ] a + v )
assert( sum = 15, "3" )
