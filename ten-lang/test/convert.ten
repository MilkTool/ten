assert( log( true ) = true, "1" )
assert( log( false ) = false, "2" )
assert( log( 123 ) = true, "3" )
assert( log( 0 ) = false, "4" )
assert( log( 1.2 ) = true, "5" )
assert( log( 0.0 ) = false, "6" )
assert( log( 'true' ) = true, "7" )
assert( log( 'false' ) = false, "8" )
assert( log( "true" ) = true, "9" )
assert( log( "false" ) = false, "10" )

assert( int( true ) = 1, "11" )
assert( int( false ) = 0, "12" )
assert( int( 123 ) = 123, "13" )
assert( int( 1.2 ) = 1, "14" )
assert( int( '123' ) = 123, "15" )
assert( int( "123" ) = 123, "16" )

assert( dec( true ) = 1.0, "17" )
assert( dec( false ) = 0.0, "18" )
assert( dec( 123 ) = 123.0, "19" )
assert( dec( 1.2 ) = 1.2, "20" )
assert( dec( '1.2' ) >= 1.19999, "21" )
assert( dec( '1.2' ) <= 1.20001, "22" )
assert( dec( "1.2" ) >= 1.19999, "23" )
assert( dec( "1.2" ) <= 1.20001, "24" )

assert( sym( true ) = 'true', "23" )
assert( sym( false ) = 'false', "24" )
assert( sym( 123 ) = '123', "25" )
assert( dec( sym( 1.2 ) ) >= 1.19999, "26" )
assert( dec( sym( 1.2 ) ) <= 1.20001, "27" )
assert( sym( 'hello' ) = 'hello', "28" )
assert( sym( "hello" ) = 'hello', "29" )


assert( bcmp( str( true ), '=', "true" ), "30" )
assert( bcmp( str( false ), '=', "false" ), "31" )
assert( bcmp( str( 123 ), '=', "123" ), "32" )
assert( dec( str( 1.2 ) ) >= 1.19999, "33" )
assert( dec( str( 1.2 ) ) <= 1.20001, "34" )
assert( bcmp( str( 'hello' ), '=', "hello" ), "35" )
assert( bcmp( str( "hello" ), '=', "hello" ), "36" )

